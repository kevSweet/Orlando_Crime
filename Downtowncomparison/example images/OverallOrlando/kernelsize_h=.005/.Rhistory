library(ggplot2)
library(ggmap)
orlando_map <- get_map(location = "Orlando", maptype = "roadmap", zoom = 14)
ggmap(orlando_map, extent = "device") + geom_point(aes(x=Longitude, y=Latitude), col = ms$labels, alpha = 0.1, size =  2, data = r.coords)
